<%= simple_form_for(@bases) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :harbour_id %>
    <%= f.input :placement %>
    <%= f.input :ip %>
    <%= f.input :last_seen %>
    <%= f.association :harbour %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
