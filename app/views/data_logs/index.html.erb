<p id="notice"><%= notice %></p>

<h1>Listing Data Logs</h1>

<table>
  <thead>
    <tr>
      <th>Device</th>
      <th>Status</th>
      <th>Battery</th>
      <th>Station timestamp</th>
      <th>Station</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_logs.each do |data_log| %>
      <tr>
        <td><%= data_log.device_id %></td>
        <td><%= data_log.status %></td>
        <td><%= data_log.battery %></td>
        <td><%= data_log.station_timestamp %></td>
        <td><%= data_log.station_id %></td>
        <td><%= link_to 'Show', data_log %></td>
        <td><%= link_to 'Edit', edit_data_log_path(data_log) %></td>
        <td><%= link_to 'Destroy', data_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data log', new_data_log_path %>
