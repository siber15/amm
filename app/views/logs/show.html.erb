<%- model_class = Log -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:device_id) %>:</strong></dt>
  <dd><%= @log.device_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @log.status %></dd>
  <dt><strong><%= model_class.human_attribute_name(:battery) %>:</strong></dt>
  <dd><%= @log.battery %></dd>
  <dt><strong><%= model_class.human_attribute_name(:station_timestamp) %>:</strong></dt>
  <dd><%= @log.station_timestamp %></dd>
  <dt><strong><%= model_class.human_attribute_name(:station_id) %>:</strong></dt>
  <dd><%= @log.station_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              logs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_log_path(@log), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              log_path(@log),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
