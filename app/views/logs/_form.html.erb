<%= simple_form_for @log, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :device_id %>
  <%= f.input :status %>
  <%= f.input :battery %>
  <%= f.input :station_timestamp %>
  <%= f.input :station_id %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                logs_path, :class => 'btn btn-default' %>
<% end %>
